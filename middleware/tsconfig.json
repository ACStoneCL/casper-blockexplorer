{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2015", "dom"],
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./src/",
    "outDir": "./dist"
  },
  "exclude": [
      "node_modules",
      "**/*.spec..ts"
  ]
}
